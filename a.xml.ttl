    wpdd:content [ a wpdd:Topic ;
            rdfs:label "Уравнения гиперболического типа."@ru ;
            wpdd:content [ a wpdd:Topic ;
                    rdfs:label "Вывод формулы Кирхгофа и Пуассона, исследование этих формул. Принцип Гюйгенса. Существование, единственность и непрерывная зависимость решений задач от начальных данных. Понятие обобщенного решения."@ru ;
                    schema:sku "2.3" ],
                [ a wpdd:Topic ;
                    rdfs:label "Применение метода Фурье для неоднородных смешанных задач для уравнений гиперболического типа."@ru ;
                    schema:sku "2.6" ],
                [ a wpdd:Topic ;
                    rdfs:label "Метод Дюамеля для решения задачи Коши в случае неоднородного волнового уравнения."@ru ;
                    schema:sku "2.2" ],
                [ a wpdd:Topic ;
                    rdfs:label "Задачи Гурса и Дарбу."@ru ;
                    schema:sku "2.4" ],
                [ a wpdd:Topic ;
                    rdfs:label "Решение однородной смешанной задачи для уравнений гиперболического типа. Собственные значения и собственные функции задачи Штурма – Лиувилля. Общая схема метода Фурье."@ru ;
                    schema:sku "2.5" ],
                [ a wpdd:Topic ;
                    rdfs:label "Волновое уравнение. Задача Коши для уравнения колебаний. Формула Даламбера."@ru ;
                    schema:sku "2.1" ] ;
            schema:sku "II" ],
        [ a wpdd:Topic ;
            rdfs:label "Уравнения эллиптического типа и теория потенциалов."@ru ;
            wpdd:content [ a wpdd:Topic ;
                    rdfs:label "Решение задачи Дирихле для уравнения Лапласа в шаре и полупространстве. Метод Фурье для решения внутренней и внешней задач Дирихле для уравнения Лапласа в круге, кольце, прямоугольнике."@ru ;
                    schema:sku "3.3" ],
                [ a wpdd:Topic ;
                    rdfs:label "Уравнения Лапласа и Пуассона. Постановка основных краевых задач. Фундаментальное решение уравнения Лапласа."@ru ;
                    schema:sku "3.1" ],
                [ a wpdd:Topic ;
                    rdfs:label "Формулы Грина. Гармонические функции и их свойства. Принцип максимума. Теоремы Лиувилля и Горнака. Функция Грина задачи Дирихле для уравнения Лапласа. Построения функции Грина для простейших областей."@ru ;
                    schema:sku "3.2" ],
                [ a wpdd:Topic ;
                    rdfs:label "Теория потенциала и сведение краевых задач для уравнений эллиптического типа к интегральным уравнениям."@ru ;
                    schema:sku "3.4" ] ;
            schema:sku "III" ],
        [ a wpdd:Topic ;
            rdfs:label "Введение."@ru ;
            wpdd:content [ a wpdd:Topic ;
                    rdfs:label "Классификация дифференциальных уравнений с частными производными. Гиперболические, параболические и эллиптические уравнения. О постановке задач для дифференциальных уравнений с частными производными. Корректная постановка задач. Примеры корректно поставленных задач. Пример Адамара некорректно поставленной задачи Коши для эллиптических уравнений."@ru ;
                    schema:sku "1.3" ],
                [ a wpdd:Topic ;
                    rdfs:label "Приведение к каноническому виду дифференциальных уравнений с частными производными. Понятие о характеристиках дифференциальных уравнений с частными производными. Связь характеристических направлений с характеристиками."@ru ;
                    schema:sku "1.4" ],
                [ a wpdd:Topic ;
                    rdfs:label "Системы уравнений с частными производными. Задача Коши. Теорема Коши – Ковалевской."@ru ;
                    schema:sku "1.5" ],
                [ a wpdd:Topic ;
                    rdfs:label "Вывод уравнений колебаний струны, теплопроводности, Лапласа, постановка краевых задач, их физическая интерпретация."@ru ;
                    schema:sku "1.2" ],
                [ a wpdd:Topic ;
                    rdfs:label "Понятие об уравнениях с частными производными. Виды уравнений. Дифференциальные с частными производными первого и второго порядков."@ru ;
                    schema:sku "1.1" ] ;
            schema:sku "I" ],
        [ a wpdd:Topic ;
            rdfs:label "Уравнения параболического типа."@ru ;
            wpdd:content [ a wpdd:Topic ;
                    rdfs:label "Применение метода Фурье для неоднородных смешанных задач для уравнений параболического"@ru ;
                    schema:sku "4.4" ],
                [ a wpdd:Topic ;
                    rdfs:label "Решение однородной смешанной задачи для уравнений гиперболического типа. Общая схема метода Фурье."@ru ;
                    schema:sku "4.3" ],
                [ a wpdd:Topic ;
                    rdfs:label "Принцип максимума в ограниченной области и решение задачи Коши, теоремы единственности и устойчивости."@ru ;
                    schema:sku "4.2" ],
                [ a wpdd:Topic ;
                    rdfs:label "Уравнение теплопроводности. Постановка краевых задач. Фундаментальное решение."@ru ;
                    schema:sku "4.1" ] ;
            schema:sku "IV" ] ;
    wpdd:courseDC [ wpdd:ableTo wpdb:f776531e-dbb0-11ec-83cd-704d7b84fd9f ;
            wpdd:aim "<p><strong>Цели</strong>: Формирование у будущих бакалавров современных теоретических знаний в области уравнений математической физики. Обзор некоторых наиболее распространенных методов исследования и решения основных классических задач для уравнений с частными производными.</p>" ;
            wpdd:competence wpdb:f77649fa-dbb0-11ec-83cd-704d7b84fd9f ;
            wpdd:know wpdb:f7765076-dbb0-11ec-83cd-704d7b84fd9f ;
            wpdd:posess wpdb:f776559e-dbb0-11ec-83cd-704d7b84fd9f ;
            wpdd:problem "Задачи: Изучить с отдельными доказательствами основные положения теории уравнений с частными производными. Дать обзор основных задач для уравнений математической физики; сформировать умение создавать математическую модель физического явления; научить студентов применять набор стандартных методов решения задач для уравнений с частными производными."@ru ;
            schema:author wpdb:f7793bd8-dbb0-11ec-83cd-704d7b84fd9f ] ;
